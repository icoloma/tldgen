--><taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd" version="2.0">
    <display-name>Loom Core Tag Library</display-name>
    <tlib-version>2.0</tlib-version>
    <short-name>loom</short-name>
    <uri>http://loom.extrema-sistemas.org/loom-core.tld</uri>
    <tag>
        <description>tag description with &lt;testing description escape</description>
        <display-name>dummy display name</display-name>
        <name>dummy</name>
        <tag-class>org.tldgen.sample.DummyTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>foo</name>
            <required>true</required>
        </attribute>
        <variable>
            <name-given>foo</name-given>
        </variable>
        <example>dummy example</example>
    </tag>
    <tag>
        <description>description of another tag</description>
        <display-name>tag2 display name</display-name>
        <name>tag2</name>
        <tag-class>org.tldgen.sample.extension.Tag2</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>foo</name>
        </attribute>
        <example>tag2 example</example>
    </tag>
    <function>
        <name>function0</name>
        <function-class>org.tldgen.functions.DummyFunction</function-class>
        <example>function0 example</example>
    </function>
    <function>
        <description>
            * int param1: Mock description
            Return: Returns true if the argument is 42, false otherwise. Go figure.
        </description>
        <name>function1</name>
        <function-class>org.tldgen.targetfunctions.FunctionSampleUtil</function-class>
        <function-signature>java.lang.String function0(int param1)</function-signature>
        <example>function1 example</example>
    </function>
</taglib>